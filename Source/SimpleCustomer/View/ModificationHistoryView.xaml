<Window x:Class="SimpleCustomer.View.ModificationHistoryView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Modifications history" Height="300" Width="650" WindowStyle="ToolWindow">
    <Grid>
        <TextBox TextWrapping="Wrap" IsReadOnly="True" BorderThickness="0" Text="{Binding ModificationHistoryText}" Margin="5"/>
    </Grid>
</Window>
